{
  "name": "codeinc/lib-router",
  "version": "1.2.2",
  "description": "Code Inc. PSR7 & PSR15 router library",
  "homepage": "https://github.com/CodeIncHQ/lib-router",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "psr/http-message": "^1.0",
    "guzzlehttp/psr7": "^1.4",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "codeinc/lib-psr7responses": "^1.0",
    "codeinc/lib-servicemanager": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "hansott/psr7-cookies": "^1.0",
    "codeinc/lib-psr7responsesender": "^1.0"
  },
  "suggest": {
    "codeinc/lib-psr7responsesender": "Required to send responses",
    "codeinc/lib-psr15middlewares": "Provides a collection of PSR15 middlewares",
    "middlewares/psr15-middlewares": "Provides another collection of PSR15 middlewares"
  },
  "autoload": {
    "psr-4": {"CodeInc\\Router\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\Router\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "repositories": [
    { "type": "vcs", "url": "https://github.com/CodeIncHQ/lib-servicemanager" }
  ]
}
