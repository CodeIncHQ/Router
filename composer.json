{
  "name": "codeinc/router",
  "version": "1.3.4",
  "description": "Code Inc. PSR7 & PSR15 router library",
  "homepage": "https://github.com/CodeIncHQ/Router",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "psr/http-message": "^1.0",
    "guzzlehttp/psr7": "^1.4",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "codeinc/lib-psr7responses": "^1.0",
    "relay/relay": "2.x@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "hansott/psr7-cookies": "^1.0",
    "codeinc/lib-psr7responsesender": "dev-master",
    "codeinc/services-manager": "dev-master"
  },
  "suggest": {
    "codeinc/lib-servicemanager": "Required to use ServiceManagerInstantiator",
    "codeinc/lib-psr7responsesender": "A PSR-7 response sender",
    "codeinc/lib-psr15middlewares": "Provides a collection of PSR15 middlewares",
    "middlewares/psr15-middlewares": "Provides another collection of PSR15 middlewares"
  },
  "autoload": {
    "psr-4": {"CodeInc\\Router\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\Router\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "repositories": [
    { "type": "vcs", "url": "https://github.com/CodeIncHQ/lib-psr7responsesender" },
    { "type": "vcs", "url": "https://github.com/CodeIncHQ/ServicesManager" }
  ]
}
