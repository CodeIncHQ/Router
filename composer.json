{
  "name": "codeinchq/lib-router",
  "version": "1.1.0",
  "description": "Code Inc. router library",
  "homepage": "https://github.com/codeinchq/lib-router",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "codeinchq/lib-url": "dev-master",
    "psr/http-message": "^1.0",
    "guzzlehttp/psr7": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "hansott/psr7-cookies": "^1.0"
  },
  "suggest": {
    "hansott/psr7-cookies": "Allows to inject cookie into the PSR7 response"
  },
  "autoload": {
    "psr-4": {"CodeInc\\Router\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\Router\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "repositories": [
    { "type": "vcs", "url": "https://github.com/codeinchq/lib-url" }
  ]
}
