{
  "name": "codeinchq/lib-router",
  "version": "1.1.0",
  "description": "Code Inc. router library",
  "homepage": "https://github.com/codeinchq/lib-router",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "codeinchq/lib-url": "dev-master",
    "codeinchq/lib-ui": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {"CodeInc\\Router\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\Router\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "repositories": [
    { "type": "vcs", "url": "https://github.com/codeinchq/lib-url" },
    { "type": "vcs", "url": "https://github.com/codeinchq/lib-ui" }
  ]
}